{
  "name": "resource-finder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lintFix": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.8.2",
    "@philly/vue-comps": "https://github.com/CityOfPhiladelphia/phila-vue-comps#1338797ae526ce690b453c9ff92b47a8466f95d0",
    "@philly/vue-datafetch": "https://github.com/CityOfPhiladelphia/phila-vue-datafetch#9f314db79a26dda9f6ac84720583256e01251cff",
    "@philly/vue-mapping": "^1.0.29",
    "axios": "^0.18.0",
    "core-js": "^2.6.5",
    "foundation-sites": "^6.5.3",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.12.0",
    "sass": "^1.19.0",
    "sass-loader": "^7.1.0",
    "style-resources-loader": "^1.2.1",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "semi": 2,
      "linebreak-style":0,
      "no-underscore-dangle": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
